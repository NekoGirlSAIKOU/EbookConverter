<DocxInputSettingUi>:
    chk_lbl_docx_no_pagebreaks_between_notes: chk_lbl_docx_no_pagebreaks_between_notes
    chk_lbl_docx_inline_subsup: chk_lbl_docx_inline_subsup
    tf_input_encoding: tf_input_encoding
    CheckboxLabel:
        id: chk_lbl_docx_no_pagebreaks_between_notes
        text: "--docx-no-pagebreaks-between-notes"
        helper_text: "Do not insert a page break after every endnote."
    CheckboxLabel:
        id: chk_lbl_docx_inline_subsup
        text: "--docx-inline-subsup"
        helper_text: "Render superscripts and subscripts so that they do not affect the line height."
    MDTextField:
        id: tf_input_encoding
        hint_text: "--input-encoding"
        helper_text: "Specify the character encoding of the input document."
        helper_text_mode: "on_focus"